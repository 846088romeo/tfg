Protocol: Secret_Goal_Pair

Types:
	Agent A,B;
	Number Msg1,Msg2;
	Function pk;		# Agent -> PublicKey

Knowledge:
	A: A,B,pk;
	B: A,B,inv(pk(B))

Actions:
	A -> B: {Msg1,Msg2}pk(B)
	B -> A: Msg1,Msg2

Goals:
	A ->* B: Msg1
	# 	{Msg}pk(B) secret between A,B
	{Msg1,Msg2}pk(B) secret between A,B
	# 	Msg secret between A,B
	Msg1,Msg2 secret between A,B
	Msg1 secret between A,B
	Msg2 secret between A,B
	inv(pk(B)) secret between B
