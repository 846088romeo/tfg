Protocol: KeyEx2

Types:
	Agent A,B,s;
	Symmetric_key KAB;
	Function shk;		# Agent,Agent ->* SymmetricKey

Knowledge:
	A: A,B,s,shk(A,s);
	B: B,A,s,shk(B,s);
	s: A,B,s,shk(A,s),shk(B,s)

Actions:
	A -> s: A,B
	s -> A: {|KAB|}shk(A,s),{|KAB|}shk(B,s)
	A -> B: A,{|KAB|}shk(B,s)

Goals:
	A authenticates s on KAB,B
	B authenticates s on KAB,A
	KAB secret between A,B,s
