Protocol: Fresh_from_A_with_DH

Types:
	Agent A,B;
	Number Msg,NxB,g,XxX,YxY;
	Function pk,sk;		# Agent -> PublicKey
	Function hash;		# Untyped -> Number

Knowledge:
	A: A,B,pk,sk,inv(sk(A)),inv(pk(A)),hash,g;
	B: A,B,pk,sk,hash,g

Actions:
	A -> B: exp(g,XxX)
	B -> A: {exp(g,YxY),NxB,B}pk(A)
	A -> B: {|NxB,hash({B,Msg}inv(sk(A)))|}exp(exp(g,XxX),YxY),{B,Msg}inv(sk(A))

Goals:
	A *-> B: Msg
	B ->* A: NxB
	A *-> B: NxB
	inv(sk(A)) secret between A
	inv(pk(A)) secret between A
