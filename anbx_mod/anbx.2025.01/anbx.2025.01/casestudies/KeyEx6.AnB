Protocol: KeyEx6

Types:
	Agent A,B,s;
	Number NA,NB;
	Symmetric_key KAB;
	Function pre;		# Number -> Number
	Function shk;		# Agent,Agent ->* SymmetricKey

Knowledge:
	A: A,B,s,pre,shk(A,s);
	B: B,A,s,pre,shk(B,s);
	s: A,B,s,pre,shk(A,s),shk(B,s)

Actions:
	B -> A: A,B,NB
	A -> s: A,B,NA,NB
	s -> A: {|KAB,B,NA|}shk(A,s),{|KAB,A,NB,s|}shk(B,s)
	A -> B: {|KAB,A,NB,s|}shk(B,s)

Goals:
	A authenticates s on KAB,B
	B authenticates s on KAB,A
	KAB secret between A,B,s
