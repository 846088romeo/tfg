Protocol: WooLam

Types:
	Agent A,B,s;
	Number NB;
	Function shk;		# Agent,Agent ->* SymmetricKey

Knowledge:
	A: A,B,s,shk(A,s);
	B: B,A,s,shk(B,s);
	s: A,B,s,shk(A,s),shk(B,s)
	where A!=B

Actions:
	A -> B: A
	B -> A: NB
	A -> B: {|NB|}shk(A,s)
	B -> s: {|A,{|NB|}shk(A,s)|}shk(B,s)
	s -> B: {|NB|}shk(B,s)

Goals:
	B authenticates A on NB
