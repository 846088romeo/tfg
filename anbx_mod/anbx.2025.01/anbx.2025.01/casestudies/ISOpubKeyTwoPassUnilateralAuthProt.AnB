Protocol: ISOpubKeyTwoPassUnilateralAuthProt

Types:
	Agent A,B,s;
	Number RA,RB,Text1,Text2,Text3,empty;
	Function sk;		# Agent -> PublicKey

Knowledge:
	A: A,B,s,sk(s),sk(A),inv(sk(A)),empty;
	B: B,s,sk(s);
	s: s,sk(s),inv(sk(s))

Actions:
	A -> s: A,sk(A)
	s -> A: {A,sk(A)}inv(sk(s))
	A -> B: empty
	B -> A: RB
	A -> B: {A,sk(A)}inv(sk(s)),{RA,RB,B,Text2}inv(sk(A))

Goals:
	A *-> B: Text2
	inv(sk(A)) secret between A
	inv(sk(s)) secret between s
