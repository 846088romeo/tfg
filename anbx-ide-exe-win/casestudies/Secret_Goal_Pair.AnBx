Protocol: Secret_Goal_Pair AnB

Types: 
	Agent A,B;
	Number Msg1,Msg2;
	Function [Agent -> PublicKey] pk
		
Definitions:
	Msg: Msg1,Msg2

Knowledge: 
	A: A,B,pk;
	B: A,B,inv(pk(B))
	
Actions:
	# A -> B: Msg
	A -> B: {Msg}pk(B)
	B -> A: Msg
	
Goals:
	A ->* B: Msg1
	{Msg}pk(B) secret between A,B # fail
	Msg secret between A,B # fail
	Msg1 secret between A,B # fail
	Msg2 secret between A,B # fail
	Msg1,Msg2 secret between A,B # fail