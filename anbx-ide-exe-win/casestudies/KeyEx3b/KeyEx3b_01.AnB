Protocol: KeyEx3b

Types:
	Agent A,B,s;
	Symmetric_key KAB;
	Function shk;		# Agent,Agent ->* SymmetricKey

Knowledge:
	A: A,B,s,shk(A,s);
	B: B,A,s,shk(B,s);
	s: A,B,s,shk(A,s),shk(B,s)

Actions:
	A -> s: A,B
	s -> A: {|KAB,A,B|}shk(A,s),{|KAB,A,B|}shk(B,s)
	A -> B: {|KAB,A,B|}shk(B,s)

Goals:
	A authenticates s on KAB,B
