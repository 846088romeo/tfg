Protocol: Secret_for_B_v2

Types:
	Agent A,B;
	Number Msg,N;
	Function pk;		# Agent -> PublicKey

Knowledge:
	A: A,B,inv(pk(A)),pk;
	B: A,B,inv(pk(B)),pk

Actions:
	B -> A: {A,B,N}pk(A)
	A -> B: {B,N,Msg}pk(B)

Goals:
	A ->* B: Msg
	Msg secret between A,B
	B authenticates A on Msg
	B authenticates A on N
	inv(pk(A)) secret between A
	inv(pk(B)) secret between B
