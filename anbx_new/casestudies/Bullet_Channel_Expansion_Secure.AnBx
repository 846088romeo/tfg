Protocol: Bullet_Channel_Expansion_Secure AnBx

# compare with Bullet_Channel example
# to allow bullet expansion it is necessary
# 1) Declare Certified agents
# 2) Set protocol interpretation to AnBx

Types:
	Agent A,B;
	Number Msg;
	Certified A,B

Knowledge:
	A: A,B;
	B: B,A

Actions:
    A *->* B: Msg

Goals:
	# A *->* B: Msg					# fail 2 sessions
	# B authenticates A on Msg		# fail 2 sessions
	
	B weakly authenticates A on Msg
	Msg secret between A,B
	